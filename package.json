{
  "name": "moneymoney",
  "version": "1.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node --max-old-space-size=8192 node_modules/.bin/vite build",
    "eslint": "eslint .",
    "preview": "vite preview",
    "release": "node src/scripts/release.js",
    "translate": "vue-i18n-extract --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/locales/**/*.?(json|yaml|yml|js)' --add true --remove true --separator '###'  --missingTranslationString 'MISSING'",
    "vitest": "vitest --passWithNoTests",
    "playwright": "playwright test --ui --output output/playwright",
    "playwright_console": "playwright test --output output/playwright",
    "playwright_coverage": "nyc --reporter=lcov --report-dir  output/coverage  playwright test"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.13.2",
    "currency-list": "^1.0.8",
    "echarts": "^6.0.0",
    "echarts-stat": "^1.2.0",
    "flag-icons": "^7.5.0",
    "lodash-es": "^4.17.21",
    "moment-timezone": "^0.6.0",
    "pdfmake": "^0.3.0-beta.8",
    "pinia": "^3.0.4",
    "roboto-fontface": "*",
    "vitest": "^4.0.15",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.3",
    "vuetify": "^3.11.2",
    "vuetify_rules": "^0.7.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "eslint": "^9.39.1",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "npm-check-updates": "^19.1.2",
    "nyc": "^17.1.0",
    "playwright": "^1.57.0",
    "vite": "^7.2.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^7.2.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-i18n-extract": "github:Spittal/vue-i18n-extract"
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ]
  }
}
